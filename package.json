{
  "name": "salesforce-wishingtree-touch",
  "productName": "salesforce-wishingtree-touch",
  "version": "0.0.1b",
  "author": "Jeremy Bunting <jeremy.bunting@spinifexgroup.com>",
  "contributors": [
    {
      "name": "Malcolm Tyson",
      "email": "malcolm.tyson@spinifexgroup.com"
    },
    {
      "name": "Jeremy Bunting",
      "email": "jeremy.bunting@spinifexgroup.com "
    }
  ],
  "description": "A touchscreen application for linux built for Salesforce Dreamforce conference",
  "license": null,
  "main": "./dist/electron/main.js",
  "scripts": {
    "build": "node .electron-vue/build.js",
    "build:darwin": "cross-env BUILD_TARGET=darwin node .electron-vue/build.js",
    "build:linux": "cross-env BUILD_TARGET=linux node .electron-vue/build.js",
    "build:mas": "cross-env BUILD_TARGET=mas node .electron-vue/build.js",
    "build:win32": "cross-env BUILD_TARGET=win32 node .electron-vue/build.js",
    "build:clean": "cross-env BUILD_TARGET=clean node .electron-vue/build.js",
    "build:web": "cross-env BUILD_TARGET=web node .electron-vue/build.js",
    "build:all": "npm run build:linux && npm run build:win32 && npm run build:darwin",
    "compress": "npm run compress:mac && npm run compress:win && npm run compress:linux",
    "compress:win": "cd build && zip -r salesforce-wishingtree-touch-win32-x64.zip salesforce-wishingtree-touch-win32-x64",
    "compress:mac": "cd build && zip -r salesforce-wishingtree-touch-darwin-x64.zip salesforce-wishingtree-touch-darwin-x64",
    "compress:linux": "cd build && zip -r salesforce-wishingtree-touch-linux-x64.zip salesforce-wishingtree-touch-linux-x64",
    "dev": "node .electron-vue/dev-runner.js",
    "pack": "npm run pack:main && npm run pack:renderer",
    "pack:main": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.main.config.js",
    "pack:renderer": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.renderer.config.js",
    "test": "npm run unit",
    "unit": "karma start test/unit/karma.conf.js",
    "postinstall": ""
  },
  "dependencies": {
    "animate.css": "^3.5.2",
    "artnet": "^1.3.3",
    "axios": "^0.16.1",
    "bootstrap": "^3.3.7",
    "chart.js": "^2.6.0",
    "dotenv": "^4.0.0",
    "electron-localshortcut": "^2.0.2",
    "font-awesome": "^4.7.0",
    "idle-vue": "^2.0.1",
    "jquery": "^3.2.1",
    "moment-timezone": "^0.5.13",
    "vue": "^2.3.2",
    "vue-chartjs": "^2.8.2",
    "vue-electron": "^1.0.6",
    "vue-router": "^2.5.3",
    "vue-template-compiler": "^2.3.2",
    "vuex": "^2.3.1"
  },
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-plugin-istanbul": "^4.1.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.24.1",
    "babili-webpack-plugin": "^0.1.2",
    "cfonts": "^1.1.3",
    "chai": "^4.0.0",
    "chalk": "^2.1.0",
    "copy-webpack-plugin": "^4.0.1",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.4",
    "del": "^3.0.0",
    "devtron": "^1.4.0",
    "electron": "^1.7.5",
    "electron-debug": "^1.4.0",
    "electron-devtools-installer": "^2.2.0",
    "electron-packager": "^8.5.0",
    "electron-rebuild": "^1.1.3",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "html-webpack-plugin": "^2.30.1",
    "inject-loader": "^3.0.0",
    "karma": "^1.3.0",
    "karma-chai": "^0.1.0",
    "karma-coverage": "^1.1.1",
    "karma-electron": "^5.1.1",
    "karma-mocha": "^1.2.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "^0.0.31",
    "karma-webpack": "^2.0.1",
    "mocha": "^3.0.2",
    "multispinner": "^0.2.1",
    "node-loader": "^0.6.0",
    "node-sass": "^4.5.3",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "url-loader": "^0.5.9",
    "vue-html-loader": "^1.2.4",
    "vue-loader": "^12.2.1",
    "vue-style-loader": "^3.0.1",
    "vue-template-compiler": "^2.4.2",
    "webpack": "^3.5.2",
    "webpack-dev-server": "^2.7.1",
    "webpack-hot-middleware": "^2.18.2",
    "webpack-merge": "^4.1.0"
  }
}
